#include <stdio.h>
#include <stdlib.h>

int initArray (int*pArray, int limite, int valor)
{
	int retorno=-1;
	int i;
	if(pArray != NULL && limite > 0)
	{
		retorno = 0;
		for(i=0;i<limite;i++)
		{
			*(pArray+i) = valor;
		}
	}
  return retorno;
}


int main(void)
{

	int* pArrayEdades = NULL;
	int sizeArrayEdades = 10;

    pArrayEdades =(int*) malloc(sizeof(int)*sizeArrayEdades);
    if(pArrayEdades != NULL)
    {

    }

	return EXIT_SUCCESS;
}
